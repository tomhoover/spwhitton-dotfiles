{
    "__comment": "permit source-only uploads",
    "-hooks": [
        "check-debs"
    ],

    "+hooks": [
        "checksums"
    ],

    "__comment": "require explicit selection of ftp-master",
    "default_host_main": "debomatic-amd64",

    "full_upload_log": true,

    "__comment": "avoid any problems with dgit (#827879)",
    "run_lintian": false
}
