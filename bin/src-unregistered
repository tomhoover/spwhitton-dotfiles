#!/bin/sh

. $HOME/.shenv

for d in $HOME/src/*; do
    if [ -d "$d" -a ! "$d" = "$HOME/src/hscripts" ]; then
        if ! grep -qe "\[$(basename $d)\]" $HOME/src/.mrconfig; then
            if ! grep -qe "\[src/$(basename $d)\]" $HOME/.mrconfig; then
                if [ ! "$d" = "$HOME/src/build-area" ]; then
                    echo $d
                fi
            fi
        fi
    fi
done
